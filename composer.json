{
    "name": "milebits/authorizer",
    "description": "Permission handling for Laravel 6.0 and up",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^6|^7|^8",
        "milebits/eloquent-filters": "^2"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Hicham SADDEK",
            "email": "h.saddek@serrahub.com",
            "homepage": "https://linkedin.com/in/hicham-saddek",
            "role": "Lead Developer"
        },
        {
            "name": "Milebits Systems",
            "email": "support@milebits.com",
            "homepage": "https://os.milebits.com",
            "role": "Project Manager"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "files": ["src/Helpers/functions.php"],
        "psr-4": {
            "Milebits\\Authorizer\\": "src/",
            "Milebits\\Authoriser\\Database\\": "database/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Milebits\\Authorizer\\Providers\\AuthorizerServiceProvider"
            ]
        }
    }
}
